		<url-pattern>/LogOutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RimuoviCliente</servlet-name>
		<url-pattern>/RimuoviCliente</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddAutoAzienda</servlet-name>
		<url-pattern>/AddAutoAzienda</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogOut</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
</web-app>