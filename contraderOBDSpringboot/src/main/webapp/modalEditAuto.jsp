<!-- Modal -->
<div class="modal fade" id="formEditAuto" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
                <form action="editAuto" method="post" role="form">
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Codice del dispositivo associato</label>
                                <input type="text" name="codDispositivo" value="${findAuto.codDispositivo}" class="form-control" disabled>
                                <input type="hidden" name="codDispositivo" value="${findAuto.codDispositivo}" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Targa</label>
                                <input type="text" name="targa" value="${findAuto.targa}" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Numero di telaio</label>
                                <input type="text" name="telaio" value="${findAuto.telaio}" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Casa costruttrice</label>
                                <input type="text" name="casaCostruttrice" value="${findAuto.casaCostruttrice}" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Modello</label>
                                <input type="text" name="modello" value="${findAuto.modello}" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Tipologia</label>
                                <input type="text" name="tipologia" value="${findAuto.tipologia}" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Alimentazione</label>
                                <input type="text" name="alimentazione" value="${findAuto.alimentazione}" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Cambio</label>
                                <select class="form-control" name="cambio">
                                    <c:if test="${findAuto.cambio == 'M'.charAt(0)}">
                                        <option value="M" selected="selected">Manuale</option>
                                        <option value="A">Automatico</option>
                                    </c:if>
                                    <c:if test="${findAuto.cambio == 'A'.charAt(0)}">
                                        <option value="M">Manuale</option>
                                        <option value="A" selected="selected">Automatico</option>
                                    </c:if>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Proprietario</label>
                                <input type="text" name="proprietario" value="${findAuto.proprietario}" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Data ultima revisione</label>
                                <input type="text" name="revisione" value="${findAuto.revisione}" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Data ultimo tagliando</label>
                                <input type="text" name="tagliandoData" value="${findAuto.tagliandoData}" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="form-group">
                                <label>Chilometri all'ultimo tagliando</label>
                                <input type="text" name="tagliandoKm" value="${findAuto.tagliandoKm}" class="form-control">
                            </div>
                        </div>
                    </div>
                    <input type="hidden" name="idOfficina" class="form-control" value="${sessionScope.model.user.id}">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Chiudi</button>
                    <button type="submit" class="btn btn-primary">
                        Conferma modifiche
                    </button>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- End Modal -->